{
  "manifest_version": 2,
  "name": "Facebook Hidden Read Post",
  "version": "0.1.2",
  "description": "隱藏在Facebook塗鴉牆上24小時內看過的貼文.",
  "icons": { 
    "16": "./icons/icon16.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png" 
  },
  "author": "Chi-Hsuan, Huang",
  "permissions": [
    "storage",
    "http://*/",
    "https://*/",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": ["jquery.min.js", "content.js"]
    }
  ]
}
